<%= form_for([@artist, @song])  do |f| %>
  Select an existing Artist:
  <%= artist_select(@song.artist, @song) %><br/>
    <br/>Or Create a new one:<br/>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br />
  <%= f.submit %>
<% end %>